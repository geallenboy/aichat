{
  "name": "aichat",
  "version": "0.0.1",
  "description": "aichat",
  "keywords": [
    "ai",
    "chatgpt",
    "nextjs",
    "openai",
    "tts",
    "stt"
  ],
  "homepage": "https://github.com/",
  "license": "MIT",
  "author": "garron",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0 \"{src,mock}/**/*.{ts,tsx}\" --fix",
    "lint:lint-staged": "lint-staged",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,html,md}\"",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.md": [
      "remark --quiet --output --",
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.json": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "stylelint --fix",
      "eslint --fix"
    ],
    "*.{ts,tsx}": [
      "prettier --parser=typescript --write",
      "stylelint --fix",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^9.5.0",
    "@lobehub/chat-plugin-sdk": "^1.32.3",
    "@lobehub/ui": "^1.138.26",
    "ahooks": "^3.7.11",
    "antd": "^5.17.3",
    "antd-style": "^3.6.2",
    "axios": "^1.7.1",
    "dayjs": "^1.11.11",
    "debug": "^4.3.4",
    "dexie": "^4.0.4",
    "fast-deep-equal": "^3.1.3",
    "hooks": "^0.3.2",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-resources-to-backend": "^1.2.1",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.378.0",
    "nanoid": "^5.0.7",
    "polished": "^4.3.1",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.1",
    "react-layout-kit": "^1.9.0",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "rtl-detect": "^1.1.2",
    "swr": "^2.2.5",
    "url-join": "^5.0.0",
    "utility-types": "^3.11.0",
    "uuid": "^9.0.1",
    "y-webrtc": "^10.3.0",
    "yjs": "^13.6.15",
    "zod": "^3.23.8",
    "zustand": "^4.5.2",
    "zustand-utils": "^1.3.2"
  },
  "devDependencies": {
    "@garron/standard": "^1.4.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/rtl-detect": "^1.0.3",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^4.2.1",
    "commitlint": "^19.2.2",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "lodash": "^4.17.21",
    "prettier": "^3.2.5",
    "stylelint": "^15.11.0",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.10"
  }
}
